void loop() {
   static int n = 0;

   if(isrflag) {
    isrflag = false;
    Serial.print("mAh: ");
    Serial.print(battery_mAh);
    Serial.print("soc: ");
    Serial.print(battery_percentage);
    Serial.print("% time: ");
    Serial.print((timeInt-lasttime)/1000000.0);
    Serial.print("s mA: ");
    Serial.println(mA);
   }
   sensors.requestTemperatures();
   sensors.getTempCByIndex(0);
}
void myISR() {
  static bool polarity;
  lasttime = timeInt;
  timeInt = micros();
  polarity = digitalRead(POL);
  if (polarity) {
    battery_mAh += ah_quanta;
    battery_percentage += percent_quanta;
  }
  else {
    battery_mAh -= ah_quanta;
    battery_percentage -= percent_quanta;
  }
  mA = 614.4/((timeInt-lasttime)/1000000.0);
  if (polarity) mA = mA * -1.0;
    isrflag = true;
  
}
